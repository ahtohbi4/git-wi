{
    "main": {
        "path": "/"
    },
    "static_page_by_all_methods": {
        "path": "/static_page/{a}/",
        "defaults": {
            "_controller": "app/modules/Default/controllers/default.js",
            "a": "b"
        },
        "requirements": {
            "a": "a|b"
        }
   },
    "static_page_by_post_method": {
        "path": "/static_page/~post",
        "defaults": {
            "_controller": "app/modules/Default/controllers/default.js"
        },
        "methods": ["post"]
    }
}
